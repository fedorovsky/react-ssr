{
  "name": "react-ssr",
  "version": "1.0.0",
  "repository": "git@github.com:fedorovsky/react-ssr.git",
  "license": "MIT",
  "author": "Anton Fedorovskyi <anton.fedorovsky@gmail.com>",
  "main": "./dist/server.js",
  "scripts": {
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "client:dev:watch": "webpack --config webpack.client.js --mode=development --watch",
    "server:dev:watch": "webpack --config webpack.server.js --mode=development --watch",
    "client:prod": "webpack --config webpack.client.js --mode=production",
    "server:prod": "webpack --config webpack.server.js --mode=production",
    "build": "npm-run-all --parallel client:prod server:prod",
    "start": "npm-run-all --parallel client:dev:watch server:dev:watch"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 9",
    "last 5 Android versions"
  ],
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.20.7",
    "@babel/plugin-transform-async-to-generator": "^7.20.7",
    "@babel/plugin-transform-regenerator": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "babel-plugin-styled-components": "^2.0.7",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.5",
    "reload": "^3.2.1",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "nodemon-webpack-plugin": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "engineStrict": true
}
